{
  "npmScope": "tailwind",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "ng-add": {
      "tags": []
    },
    "ng-add-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-add"]
    },
    "nx-setup": {
      "tags": []
    },
    "nx-setup-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-setup"]
    }
  }
}
